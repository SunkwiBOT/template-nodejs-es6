{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "plugin:no-template-curly-in-string-fix/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": "off",
    "no-console": 1
  }
}
